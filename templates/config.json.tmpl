{
    "kbase": {
        "services": {
            "Auth2": {
                "url": "https://{{ .Env.KBASE_SECURE_CONFIG_PARAM_SERVICE_HOST }}/services/auth/api/legacy/KBase/Sessions/Login"
            },
            "ServiceWizard": {
                "url": "https://{{ .Env.KBASE_SECURE_CONFIG_PARAM_SERVICE_HOST }}/services/service_wizard"
            }
        },
        "baseURL": "https://{{ .Env.KBASE_SECURE_CONFIG_PARAM_UI_HOST }}"
    },
    "env": {
        "CLIENT_ID": "{{ .Env.KBASE_SECURE_CONFIG_PARAM_ORCID_CLIENT_ID }}",
        "CLIENT_SECRET": "{{ .Env.KBASE_SECURE_CONFIG_PARAM_ORCID_CLIENT_SECRET }}",
        "IS_DYNAMIC_SERVICE": "{{ default "yes" .Env.KBASE_SECURE_CONFIG_PARAM_IS_DYNAMIC_SERVICE }}"
    }
}
